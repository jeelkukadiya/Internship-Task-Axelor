<?xml version="1.0" encoding="UTF-8"?>
<!-- modules/axelor-contact/src/main/resources/domains/Title.xml -->
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_7.3.xsd">

  <module name="contact" package="com.axelor.contact.db"/>

  <entity name="Title" cacheable="true">
    <string name="name" required="true" unique="true" min="2" title="Name"/>
    <string name="code" required="true" unique="true" min="2" namecolumn="true" title="Code"/>
  </entity>

</domain-models>