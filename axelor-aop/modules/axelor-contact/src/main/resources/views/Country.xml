<?xml version="1.0" encoding="UTF-8"?>
<!-- modules/axelor-contact/src/main/resources/views/Country.xml -->
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  https://axelor.com/xml/ns/object-views/object-views_7.3.xsd">
  


  <grid name="country-grid" title="Countries" model="com.axelor.contact.db.Country">
    <field name="name"/>
    <field name="code"/>
  </grid>

  <form name="country-form" title="Country" model="com.axelor.contact.db.Country">
    <panel title="Overview">
      <field name="name"/>
      <field name="code"/>
    </panel>
  </form>

</object-views>